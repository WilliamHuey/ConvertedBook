{
  "author": "William Huey @WilliamHuey",
  "bin": {
    "convertedbook": "./bin/run.js"
  },
  "dependencies": {
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5.4.46",
    "fast-glob": "^3.3.3",
    "get-port": "^7.1.0",
    "inflection": "^3.0.2",
    "is-there": "^4.5.2",
    "listify": "^1.0.3",
    "lookpath": "^1.2.3",
    "nyc": "^17.1.0",
    "path-exists": "^5.0.0",
    "playwright": "^1.54.2",
    "ramda": "^0.31.3",
    "rxjs": "^7.8.2",
    "ts-pattern": "^5.8.0",
    "ts-xor": "^1.3.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.4"
  },
  "description": "A live reload server previewer for working with LaTeX documents. Syncs document changes to a web page in the browser to visualize rendered changes. Convert to multiple different output formats (html, pdf, epub).",
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/listify": "^1.0.2",
    "@types/node": "^24",
    "@types/ramda": "^0.31.0",
    "@vitest/coverage-v8": "^3.2.4",
    "del": "^8.0.0",
    "eslint": "^9.33.0",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10.1.8",
    "is-online": "^11.0.0",
    "oclif": "^4.22.9",
    "shx": "^0.4.0",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/WilliamHuey/convertedbook",
  "license": "MIT",
  "main": "",
  "name": "convertedbook",
  "oclif": {
    "bin": "convertedbook",
    "dirname": "convertedbook",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " "
  },
  "repository": "WilliamHuey/convertedbook",
  "scripts": {
    "build": "tsx ./src/functions/generate/generate-remove-files.ts && shx rm -rf dist && tsc -b && tsx ./src/functions/generate/generate-copy-files.ts",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "npm run test:nodownloads && npm run test:downloads",
    "test:all:shard": "vitest run --shard=1/4 --coverage --no-typecheck",
    "test:build": "vitest run test/commands/build.test.ts --coverage --no-typecheck",
    "test:generate": "vitest run test/commands/generate.test.ts --coverage --no-typecheck",
    "test:nodownloads": "vitest run test/commands/*.test.ts --coverage --no-typecheck",
    "test:downloadbuild": "vitest run test/commands-download/build.test.ts --coverage",
    "test:downloadgenerate": "vitest run test/commands-download/generate.test.ts --coverage",
    "test:downloads": "vitest run test/commands-download/*.test.ts --coverage",
    "test:merge": "nyc merge coverage ./coverage/coverage-final.json",
    "test:report": "nyc report --reporter=text --report-dir=coverage --temp-dir=coverage",
    "readme": "npm run build && oclif readme --no-aliases"
  },
  "version": "1.0.0",
  "bugs": "https://github.com/WilliamHuey/convertedbook/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts",
  "exports": "./lib/index.js",
  "type": "module"
}
