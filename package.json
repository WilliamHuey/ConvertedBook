{
  "name": "convertedbook",
  "description": "Convert latex to different ebook formats",
  "version": "1.0.0",
  "author": "William Huey",
  "bin": {
    "convertedbook": "bin/run"
  },
  "bugs": "https://github.com/WilliamHuey/ConvertedBook/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.0",
    "@types/ramda": "^0.27.23",
    "file-type": "^15.0.1",
    "is-there": "^4.5.1",
    "is-what": "^3.11.2",
    "listify": "^1.0.3",
    "lookpath": "^1.1.0",
    "plur": "^4.0.0",
    "ramda": "^0.27.1",
    "rxjs": "^6.6.3",
    "ts-pattern": "^1.1.0",
    "ts-xor": "^1.0.8",
    "tslib": "^1.14.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.7",
    "@types/chai": "^4.2.13",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.37",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "is-core-module": "^2.1.0",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "ts-plato": "^0.0.6",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/WilliamHuey/ConvertedBook",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "convertedbook",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "WilliamHuey/ConvertedBook",
  "scripts": {
    "complex": "./node_modules/.bin/ts-plato -r -d ./report src",
    "postpack": "rm -f oclif.manifest.json",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "nyctest": "nyc --extension .ts  mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
