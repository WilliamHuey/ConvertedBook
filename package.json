{
  "name": "convertedbook",
  "description": "Convert LaTeX to different ebook formats and includes live server functionality to preview LaTeX changes.",
  "version": "1.0.0",
  "author": "William Huey",
  "bin": {
    "convertedbook": "bin/run"
  },
  "bugs": "https://github.com/WilliamHuey/ConvertedBook/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.3",
    "@rxnode/core": "^0.5.3",
    "@rxnode/fs": "^0.5.4",
    "inflection": "^1.13.1",
    "is-there": "^4.5.1",
    "listify": "^1.0.3",
    "lookpath": "^1.2.2",
    "module-alias": "^2.2.2",
    "playwright": "^1.17.1",
    "port-scout": "^1.1.2",
    "ramda": "^0.27.1",
    "rxjs": "^7.4.0",
    "ts-pattern": "^3.3.3",
    "ts-xor": "^1.0.8",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.7",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^5.2.7",
    "@types/node": "^16.11.7",
    "@types/ramda": "^0.27.49",
    "chai": "^4.3.4",
    "concurrently": "^6.4.0",
    "del": "^6.0.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "is-core-module": "^2.7.0",
    "is-online": "^9.0.1",
    "mocha": "^5.2.0",
    "nyc": "^15.1.0",
    "sabik": "0.0.14",
    "ts-node": "^10.3.0",
    "typescript": "^4.5.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/WilliamHuey/ConvertedBook",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "convertedbook",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "WilliamHuey/ConvertedBook",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "concurrently \"npm run test-commands\" \"npm run test-commands-download\"",
    "test-commands": "mocha \"test/commands/*.test.ts\"",
    "test-commands-download": "mocha \"test/commands-download/*.test.ts\"",
    "nyctest": "concurrently -m 1 \"npm run nyctest-commands-download\" \"npm run nyctest-commands\"",
    "nyctest-commands": "nyc --extension .ts  mocha \"test/commands/*.test.ts\"",
    "nyctest-commands-download": "nyc --extension .ts  mocha \"test/commands-download/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "_moduleAliases": {
    "@utilities": "src/utilities"
  }
}