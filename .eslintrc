{
  "extends": [
    "oclif",
    "oclif-typescript"
  ],
  "rules": {
    "max-params": [
      1,
      6
    ],
    "max-nested-callbacks": [
      1,
      5
    ],
    "object-curly-spacing": [
      "error",
      "always"
    ],
    "indent": [
      "error",
      2,
      {
        "MemberExpression": 1
      }
    ],
    "one-var": 0,
    "no-console": 0,
    "comma-dangle": 0,
    "semi": [
      2,
      "always"
    ]
  }
}